<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_my_orders"
    app:startDestination="@id/orderList">

    <fragment
        android:id="@+id/orderList"
        android:name="com.example.pocketmoney.shopping.ui.customernavigation.OrderList"
        android:label="fragment_order_list"
        tools:layout="@layout/fragment_order_list" >
        <action
            android:id="@+id/action_orderList_to_orderDetails"
            app:destination="@id/orderDetails" />
    </fragment>
    <fragment
        android:id="@+id/orderDetails"
        android:name="com.example.pocketmoney.shopping.ui.customernavigation.OrderDetails"
        android:label="fragment_order_details"
        tools:layout="@layout/fragment_order_details" >
        <argument
            android:name="orderNumber"
            app:argType="string" />
    </fragment>
</navigation>